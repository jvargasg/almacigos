{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Contado</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Iva</th>
                <td>{{ entity.iva }}</td>
            </tr>
            <tr>
                <th>Porcentajedeltotal</th>
                <td>{{ entity.porcentajeDelTotal }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('contado') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('contado_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
